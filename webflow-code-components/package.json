    "@webflow/react": "^1.2.0",
    "@webflow/webflow-cli": "^1.12.0",
    "copyfiles": "^1.0.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=24.3.0"
  }
}
